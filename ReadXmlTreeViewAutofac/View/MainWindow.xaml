<Window x:Class="ReadXmlTreeViewAutofac.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:system="clr-namespace:System;assembly=mscorlib"
        xmlns:viewModel="clr-namespace:ReadXmlTreeViewAutofac.ViewModel"
        Title="MainWindow" Height="350" Width="525">
    <Window.Resources>
        <viewModel:TreeViewModel x:Key="TreeViewModel" />
    </Window.Resources>
    <TabControl>
        <TabItem Header="Without Dependency Injection">
            <Grid DataContext="{StaticResource TreeViewModel}">
                <StackPanel >
                    <TreeView Name="tv" ItemsSource="{Binding Path=TreeViewModels}" Height="161"/>
                </StackPanel>
            </Grid>
        </TabItem>
    </TabControl>
</Window>